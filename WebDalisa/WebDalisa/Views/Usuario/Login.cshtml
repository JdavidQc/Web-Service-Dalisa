@model WebDalisa.Models.UsuarioDTO


<style>
    body {
        height: 100vh;
        background-color: #707e97;
        background: -webkit-linear-gradient(#707e97, #ffff00b9);
        background: -o-linear-gradient(#707e97, #ffff00b9);
        background: -moz-linear-gradient(#707e97, #ffff00b9);
        background: linear-gradient(#707e97, #ffff00b9);
    }

    h1 {
        font-family: "Pacifico", cursive;
        color: gray;
    }

    #email {
        color: rgb(133, 133, 133);
        text-decoration: none;
    }

        #email:hover {
            color: #000000;
        }



</style>

<div class="container-fluid">
    <br />
    <br />
    <div class="card m-auto rounded" style="width: 25rem; border: 2px solid white">
        <div style="background: #206f7b; border-bottom: 0px; padding: 0px">
            <img src="data:image/webp;base64,"
                 class="w-100" alt="..." />
        </div>
        <div class="card-body w-75 m-auto">
            <h1 class="card-title pb-4 text-center">
                D A L I S A
            </h1> <br>

            @using (Html.BeginForm())
            {
                @Html.AntiForgeryToken()

                <div class="ps-4">
                    <div class="row pb-2">
                        <div class="col-2">
                            <img src="data:image/webp;base64,"
                               alt="" />
                        </div>
                        <div class="col-10">
                            <input type="text" name="use" class="form-control" placeholder="User / Usuario"
                                   id="exampleInputUsuario" />
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-2">
                            <img src="data:image/webp;base64,"
                               alt="" />
                        </div>
                        <div class="col-10">
                            <input type="password" name="pass" class="form-control" placeholder="Password / Contrasena"
                                   id="exampleInputPassword" />
                        </div>
                    </div>
                    <br />
                </div><br>
                <div class="d-grid gap-2 col-11 ps-4">
                    <input type="submit" value="Ingresar" style="background: #206f7b; border-color: #206f7b" class="btn  btn-primary" />
                </div>}
            <br />
            <div class="col-12">
                <p class="text-center" style="font-size: 14px">
                    <a id="email"href="http://localhost:64395/Usuario/EnvioCorreo">Recover Password-Recuperar Contraseña</a>
                </p>
            </div>
        </div><br>
        <div style="background: #e5e5e5">
            <p class="pt-3 pe-3" style="text-align: right; color: #b3b0b0">
                Operated by Netzen
            </p>
        </div>
    </div>
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
